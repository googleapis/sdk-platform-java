import com.google.showcase.v1beta1.ProfileName;

public class CreateBlurbProfileNameParentByteStringImage {

  public static void main(String[] args) throws Exception {
    createBlurbProfileNameParentByteStringImage();
  }

  public static void createBlurbProfileNameParentByteStringImage() throws Exception {
    try (MessagingClient messagingClient = MessagingClient.create()) {
      ProfileName parent = ProfileName.of("[USER]");
      ByteString image = ByteString.EMPTY;
