            },
            "ListShelves": {
              "methods": ["listShelves", "listShelvesPagedCallable", "listShelvesCallable"]
            },
            "MergeShelves": {
              "methods": ["mergeShelves", "mergeShelves", "mergeShelves", "mergeShelves", "mergeShelves", "mergeShelvesCallable"]
            },
            "MoveBook": {
