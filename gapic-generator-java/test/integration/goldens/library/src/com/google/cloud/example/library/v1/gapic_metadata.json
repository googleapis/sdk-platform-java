{
  "schema": "1.0",
  "comment": "This file maps proto services/RPCs to the corresponding library clients/methods",
  "language": "java",
  "protoPackage": "google.example.library.v1",
  "libraryPackage": "com.google.cloud.example.library.v1",
  "services": {
    "LibraryService": {
      "clients": {
