{
  "extends": [
    "config:recommended"
  ],
  "dependencyDashboard": true,
  "ignoreDeps": [
    "rules_pkg"
  ],
  "ignorePaths": [
    "^library_generation/requirements\\.txt$"
  ],
  "bazel": {
    "enabled": false
  },
  "cloudbuild": {
    "enabled": false
  },
  "dockerfile": {
    "enabled": false
  },
  "gradle": {
    "enabled": false
  },
  "gradle-wrapper": {
    "enabled": false
  },
  "maven": {
    "enabled": false
  },
  "pip_setup": {
    "enabled": false
  },
  "pip_requirements": {
    "enabled": false
  },
  "github-actions": {
    "enabled": false
  },
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["/^dependencies\\.txt$/"],
      "matchStrings": ["(?<depName>.*),(.*)=(?<currentValue>.*)"],
      "datasourceTemplate": "maven"
    }
  ]
}
